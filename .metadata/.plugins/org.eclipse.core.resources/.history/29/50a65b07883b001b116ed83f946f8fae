package com.gcha.datastructure.stack;

public class Stack {

	//후입선출
//	int 배열을 사용해서 정수를 저장하는 Stack을 구현하세요.
//	void push(int data)를 구현하세요.
//	int pop()을 구현하세요.
	
	private int[] stackArr;
	private int topIndex;
	
	Stack(){
		this.stackArr = new int[10];
		this.topIndex = -1;
	}
	
	//data 추가
	void push() {
		this.topIndex += 1;
		
		for(int i=0; i < topIndex ; i++) {
			
		}
		
	}
	
	//data 제거
	void pop() {
		topIndex -= 1;
		
	}
}
